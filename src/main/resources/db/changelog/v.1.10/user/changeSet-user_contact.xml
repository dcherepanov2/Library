<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet author="Danya (generated)" id="610">
        <preConditions onFail="MARK_RAN">
            <not>
                <tableExists tableName="user_contact"/>
            </not>
        </preConditions>
        <createTable tableName="user_contact">
            <column name="id" type="INT">
                <constraints nullable="false" unique="true"/>
            </column>
            <column name="user_id" type="INT">
            </column>
            <column name="type" type="TINYINT">
                <constraints nullable="false"/>
            </column>
            <column name="approved" type="TINYINT">
                <constraints nullable="false"/>
            </column>
            <column name="code" type="BIGINT"/>
            <column name="code_trials" type="INT"/>
            <column name="codetime" type="DATETIME"/>
            <column name="contact" type="VARCHAR(255)">
                <constraints nullable="false"/>
            </column>
        </createTable>
        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableCatalogName="user_contact"
                                 baseTableName="user_contact"
                                 constraintName="fk_user_id"
                                 onDelete="CASCADE"
                                 onUpdate="CASCADE"
                                 referencedColumnNames="id"
                                 referencedTableCatalogName="user_contact"
                                 referencedTableName="users"/>
    </changeSet>

</databaseChangeLog>